{
  "name": "knights-tour",
  "version": "1.0.0",
  "description": "A knight's tour is a sequence of moves of a knight on a chessboard such that the knight visits every square only once.",
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "NODE_PATH=source && babel-node devServer.js",
    "server": "NODE_PATH=source && babel-node source/server/index.js",
    "test": "NODE_PATH=source && babel-node source/test/index.js",
    "lint": "eslint source",
    "watch": "watch 'clear && npm run lint -s && npm run test -s' source",
    "check": "npm run lint && npm run test && npm outdated --depth=0",
    "update": "updtr"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:gacosta89/knightsTour.git"
  },
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-core": "6.3.17",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.0",
    "babel-plugin-react-transform": "1.1.1",
    "babel-plugin-transform-runtime": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "cheerio": "0.19.0",
    "css-loader": "^0.23.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.11.3",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "rimraf": "2.4.4",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "tap-xunit": "1.2.1",
    "tape": "4.2.2",
    "updtr": "0.1.6",
    "watch": "0.16.0",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0"
  },
  "dependencies": {
    "express": "4.13.3",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "react-redux": "4.0.1",
    "redux": "^3.0.5"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gacosta89/knightsTour/issues"
  },
  "homepage": "https://github.com/gacosta89/knightsTour"
}
